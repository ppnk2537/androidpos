<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
     <item android:state_pressed="true" >
    <shape>
      <solid
          android:color="#003366" />
      <stroke
          android:width="1dp"
          android:color="#001933" />
    </shape>
  </item>
  <item>
    <shape>
      <gradient
          android:startColor="#003366"
          android:endColor="#001933"
          android:angle="270" />
      <stroke
          android:width="1dp"
          android:color="#001933" />
 
    </shape>
  </item>
</selector>
