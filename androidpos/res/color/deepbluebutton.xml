<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:state_pressed="true"><shape>
            <solid android:color="#003366" />

            <stroke android:width="1dp" android:color="#001933" />
        </shape></item>
    <item><shape>
            <gradient android:angle="270"  android:centerColor="#003366" android:endColor="#001933" android:startColor="#001933" />

            <padding android:bottom="20dp" android:left="20dp" android:right="20dp" android:top="20dp" />

            <stroke android:width="1dp" android:color="#001933" />
            
            <corners android:radius= "8dp" />
        </shape></item>

</selector>